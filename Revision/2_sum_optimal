#include<bits/stdc++.h>
using namespace std ;

vector<int> Twosum_optimal(vector<int>v,int target){
    int n = v.size();
        vector<pair<int, int>> numsWithIndex;
        for (int i = 0; i < n; i++) {
            numsWithIndex.push_back({v[i], i});
        }
        sort(numsWithIndex.begin(), numsWithIndex.end());


        int left = 0, right = n - 1;

    while(left>right){

        int sum = v[i] +v[j] ;
        if(sum==target){
            return {left,right} ;
        }else if(sum<target){
            i++ ;
        }
        else{
            j-- ;
        }




    }
    return {-1,1}

}

int main() {
    int n ;
    cin>>n ;
    vector<int>arr(n);

    for(int i =0 ;i<n;i++){
        cin>>arr[i];


    }

}

